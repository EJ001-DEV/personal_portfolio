{
  "builds": [
    {
      "src": "django_portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb" , 
        "runtime": "python3.9" 
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "static_build"
      }
    }
],
  "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },    
      {
          "src": "/(.*)",
          "dest": "django_portfolio/wsgi.py"
      }
  ]
}