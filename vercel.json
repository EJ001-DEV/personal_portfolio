{
  "builds": [{
      "src": "django_portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" , "runtime": "python3.9"}
  }],
  "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },      
      {
          "src": "/(.*)",
          "dest": "django_portfolio/wsgi.py"
      }
  ]
}