{
  "version": 2,
  "builds": [
    {
      "src": "django_portfolio/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb" , 
        "runtime": "python3.9" 
      }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
]
}